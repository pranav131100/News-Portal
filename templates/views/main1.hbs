<script>


let date = new Date();
let d1 = date.getDate();
let m1 = date.getMonth();

var month = new Array();
month[0] = "Jan";
month[1] = "Feb";
month[2] = "Mar";
month[3] = "Apr";
month[4] = "May";
month[5] = "June";
month[6] = "July";
